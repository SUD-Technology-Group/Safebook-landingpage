{{> section/banner  banners=banners}}
  <div class='row row-dashed show-for-small' id='row-152207476'>
    <div class='col small-12 large-12'>
      <div class='col-inner'>
        <h2
          style="margin-bottom: 20px; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif; text-align: center;"
        ><span style='font-size: 120%;'>PHẦN MỀM KẾ TOÁN</span></h2>
        <div class='row' id='row-227837639'>
          <div class='col medium-6 small-12 large-6'>
            <div class='col-inner'>
              <div class='img has-hover x md-x lg-x y md-y lg-y' id='image_1277565962'>
                <div class='img-inner dark'>
                  <img
                    alt=''
                    class='attachment-large size-large'
                    height='343'
                    loading='lazy'
                    src='/images/simple-facebook-mb-min-min.png'
                    width='508'
                  /></div>
              </div>
            </div>
          </div>
          <div class='col medium-6 small-12 large-6'>
            <div class='col-inner'>
              <div class='img has-hover x md-x lg-x y md-y lg-y' id='image_1642314036'>
                <div class='img-inner dark'>
                  <img
                    alt=''
                    class='attachment-large size-large'
                    height='343'
                    loading='lazy'
                    src='/images/pc-min.png'
                    width='508'
                  /></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

{{#each sections}}
  {{#when layout 'eq'  'Liên kết'}}
    {{> section/headingSection title=title href=link }}
  {{/when}}

  {{#when layout 'eq'  'Hỗ trợ'}}
     {{> section/contactSection title=title contact=section message=../message }}
  {{/when}}
{{/each}}

  <div class='bg section-bg fill bg-fill bg-loaded'></div>
  <div class='section-content relative'>
    <div class='gap-element clearfix' style='display:block; height:auto; padding-top:50px'></div>
      {{#each sections}}
          {{#when layout 'eq' 'Giới thiệu'}}
             {{> section/introSection title=title description=description }}
          {{/when}}
      {{/each}}

      {{#each sections}}
        {{#when layout 'eq' 'Nội dung'}}
          {{#when @index 'eq' 4}}
              {{> section/contentSection title=title  description=description image=image }}
          {{/when}}
          {{#when @index 'eq' 5}}

            {{> section/contentSection title=title reserved=true description=description image=image }}
            {{/when}}
          {{/when}}
      {{/each}}
    </div>
  </div>

{{> section/otherSection }}
{{> section/slideSection customers=customers }}

{{#each sections}}
  {{#when layout 'eq' 'Đội ngũ'}}
    {{> section/staffSection  staffs=../staffs  title=title description=description  }}
  {{/when}}
{{/each}}

{{#each sections}}
  {{#when layout 'eq' 'Bình luận'}}
    {{> section/feedbackSection  title=title description=description comments=../comments}}
  {{/when}}
{{/each}}

{{#each sections}}
  {{#when layout 'eq' 'Video'}}
    {{> section/videoSection title=title description=description href=link}}
  {{/when}}
{{/each}} 
